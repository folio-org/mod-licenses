          "methods": ["DELETE"],
          "pathPattern": "/licenses/custprops/{id}",
          "permissionsRequired": [ "licenses.custprops.item.delete" ]
        },
        
        {
          "methods": ["GET"],
          "pathPattern": "/licenses/licenseLinks",
