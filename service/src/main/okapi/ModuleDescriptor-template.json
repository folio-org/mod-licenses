          "methods": ["GET"],
          "pathPattern": "/licenses/refdata",
          "permissionsRequired": [ "licenses.refdata.collection.get" ]
        },{
          "methods": ["GET"],
          "pathPattern": "/licenses/refdata/{domain}/{property}",
          "permissionsRequired": [ "licenses.refdata.collection.get" ]
        },{
          "methods": ["GET"],
          "pathPattern": "/licenses/refdata/{id}",
          "permissionsRequired": [ "licenses.refdata.item.get" ]
