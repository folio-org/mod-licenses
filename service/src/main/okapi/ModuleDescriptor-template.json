    {
      "id": "dashboard",
      "interfaceType": "multiple",
      "version": "1.0",
      "handlers": [
        {
          "methods": [ "GET" ],
          "pathPattern": "/dashboard/definitions",
          "permissionsRequired": []
        }
      ]
    },
    {
      "id" : "_tenant",
      "version" : "1.2",
      "interfaceType" : "system",
      "handlers" : [ {
          "methods" : [ "POST", "DELETE" ],
          "pathPattern" : "/_/tenant"
        }, {
         "methods" : [ "POST" ],
         "pathPattern" : "/_/tenant/disable"
        }
      ]
    }
  ],
