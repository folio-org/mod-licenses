        },{
          "methods": ["DELETE"],
          "pathPattern": "/licenses/licenses/{id}",
          "permissionsRequired": [ "licenses.licenses.item.delete" ],
          "modulePermissions": [ "erm.agreements.linkedLicenses.get" ]
        },
        {
          "methods": ["GET"],
          "pathPattern": "/licenses/amendments",
