    {
      "id": "licenses",
      "version": "${info.app.minorVersion}",
      "handlers" : [
        {
          "methods": ["GET"],
          "pathPattern": "/licenses/licenses",
          "permissionsRequired": [ "licenses.licenses.collection.get" ]
