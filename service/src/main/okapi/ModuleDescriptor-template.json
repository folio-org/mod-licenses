        },{
          "methods": ["GET"],
          "pathPattern": "/licenses/licenseLinks/{id}",
          "permissionsRequired": [ "licenses.licenseLinks.item.get" ]
        },
        
        {
          "methods": ["POST"],
          "pathPattern": "/licenses/licenses/compareTerms",
          "permissionsRequired": [ "licenses.compareTerms" ]
        }
      ]
